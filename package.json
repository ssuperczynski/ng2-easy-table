{
  "name": "ngx-easy-table-demo",
  "version": "1.0.0",
  "description": "ngx-easy-table demo",
  "typings": "index.ts",
  "repository": "https://github.com/ssuperczynski/ngx-easy-table.git",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build demo --prod --configuration=production",
    "build-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --prod --configuration=production",
    "lint": "ng lint",
    "full-test": "npm run lint && npm run build:lib && npm run build && npm run cy:ci",
    "e2e": "npm run cy:ci",
    "cy:serve": "http-server dist/demo -p 4201",
    "cy:local": "ng serve --port=4201",
    "cy:test": "cypress open",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:ci": "start-server-and-test cy:serve http://localhost:4201 cy:run",
    "watch:table": "ng build ngx-easy-table --watch",
    "publish:lib": "cd dist/ngx-easy-table && npm publish",
    "publish:lib:next": "cd dist/ngx-easy-table && npm publish --tag next",
    "build:lib": "ng build ngx-easy-table && node-sass projects/ngx-easy-table/assets/style.scss dist/ngx-easy-table/style.css && cp projects/ngx-easy-table/assets/style.scss dist/ngx-easy-table/style.scss"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "ng lint"
    }
  },
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.2",
    "export-to-csv": "^0.2.1",
    "highlight.js": "^9.17.1",
    "ngx-easy-table": "^11.3.1",
    "ngx-highlightjs": "^4.0.2",
    "spectre.css": "^0.5.8",
    "typeface-montserrat": "0.0.75"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.24",
    "@angular-devkit/build-ng-packagr": "~0.803.24",
    "@angular/cdk": "^8.2.2",
    "@angular/cli": "^8.3.25",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@types/faker": "^4.1.9",
    "@types/node": "^12.12.26",
    "bootstrap": "^4.4.1",
    "codelyzer": "^5.2.1",
    "cypress": "^3.8.3",
    "faker": "^4.1.0",
    "http-server": "^0.12.0",
    "husky": "^3.1.0",
    "ng-packagr": "^5.7.1",
    "node-sass": "^4.13.0",
    "rxjs": "^6.5.4",
    "start-server-and-test": "^1.10.8",
    "ts-node": "^8.5.4",
    "tsickle": "^0.37.1",
    "tslib": "^1.10.0",
    "tslint": "^5.20.1",
    "tslint-sonarts": "^1.9.0",
    "typescript": "~3.5.3",
    "zone.js": "~0.10.2"
  },
  "engines": {
    "node": ">=11.0.0",
    "npm": ">=6.0.0"
  },
  "engineStrict": true,
  "bugs": {
    "url": "https://github.com/ssuperczynski/ngx-easy-table/issues"
  },
  "homepage": "https://github.com/ssuperczynski/ngx-easy-table",
  "main": "main.js",
  "keywords": [
    "angular",
    "angular2",
    "angular6",
    "angular7",
    "angular8",
    "table",
    "angular table",
    "angular2 table",
    "angular 2 table",
    "angular-table",
    "angular2-table",
    "ngx-easy-table"
  ],
  "author": "Sebastian Superczynski <s.superczynski@gmail.com>"
}
